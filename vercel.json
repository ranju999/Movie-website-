{
    "version": 2,
    "builds": [
        {
            "src": "api/add-movie.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/add-movie",
            "dest": "/api/add-movie.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}
